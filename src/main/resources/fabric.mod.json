{
	"schemaVersion": 1,
	"id": "skord",
	"version": "${version}",
	"name": "Skord",
	"description": "This mod is an attempt to bring IC2 style farming to modern minecraft.",
	"authors": [
		"Antonic"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/skord/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.antonic.skord.Skord"
		],
		"fabric-datagen": [
			"com.antonic.skord.SkordDataGenerator"
		],
		"client": [
			"com.antonic.skord.SkordClient"
		],
		"jade": [
			"com.antonic.skord.compat.jade.ModJadePlugin"
		],
		"rei_client": [
			"com.antonic.skord.compat.rei.ModREIClientPlugin"
		]
